  "homepage": "https://useful.laurenblack.co.uk/",
  "support": {
    "docs": "https://useful.laurenblack.co.uk/docs",
    "issues": "https://github.com/UsefulPHP/Project/issues",
    "source": "https://github.com/UsefulPHP/Project",
    "rss": "https://github.com/UsefulPHP/Project/releases.atom"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^7.4"
  },
  "require-dev": {
    "useful/installer": "@stable",
    "phpunit/phpunit": "^9.2.2",
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {
